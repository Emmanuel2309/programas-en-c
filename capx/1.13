#include <stdio.h>
/* AplicaciÃ³n de operadores. */
void main(void)
{
    int i = 5, j = 4, k, l, m;

    k = !i * 3 + --j * 2 - 3;
    printf("\nEl valor de k es: %d", k); // Se muestra en pantalla el valor de k (2)

    l = !(!i || 1 && 0) && 1;
    printf("\nEl valor de l es: %d", l); // Se muestra en pantalla el valor de l (1)

    m = (k = (! (12 > 10)), j = (10 || 0) && k, (! (k || j)));
    printf("\nEl valor de m es: %d", m); // Se muestra en pantalla el valor de m (1)
}
